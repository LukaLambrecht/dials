            "in": "query",
            "name": "ls_id",
            "schema": {
              "type": "integer",
              "maximum": 9223372036854776000,
              "minimum": -9223372036854776000,
              "format": "int64"
            }
          },
          {
            "in": "query",
            "name": "ls_number",
            "schema": {
              "type": "integer"
            },
            "description": "Lumisection number"
          },
          {
            "in": "query",
            "name": "max_ls_number",
            "schema": {
              "type": "integer"
            },
            "description": "Maximum lumisection number"
          },
          {
            "in": "query",
            "name": "max_run_number",
            "schema": {
              "type": "integer"
            },
            "description": "Maximum run number"
          },
          {
            "in": "query",
            "name": "min_entries",
            "schema": {
              "type": "integer"
            },
            "description": "Minimum number of entries"
          },
          {
            "in": "query",
            "name": "min_ls_number",
            "schema": {
