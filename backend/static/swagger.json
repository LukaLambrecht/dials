              "type": "string"
            }
          },
          {
            "in": "query",
            "name": "title_contains",
            "schema": {
              "type": "string"
            },
            "description": "Title contains"
          },
          {
            "in": "header",
            "name": "workspace",
            "schema": {
              "type": "string"
            },
            "description": "Workspace"
          }
        ],
        "tags": [
